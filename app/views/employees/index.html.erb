<h1>Empleados</h1>


<table class="table">
  <tr>
    <th>Name</th>
    <th>Rut</th>
    <th>Position</th>
    <th>Birthday</th>
    <th>Phone</th>
    <th>Start date</th>
    <th></th>
  </tr>
 
  <% @employees.each do |employee| %>
    <tr>
      <td><%= employee.name %></td>
      <td><%= employee.rut %></td>
      <td><%= employee.position %></td>
      <td><%= employee.birthday %></td>
      <td><%= employee.phone %></td>
      <td><%= employee.start_date %></td>
      <td><%= link_to 'Show', employee_path(employee) %></td>
      <td><%= link_to 'Edit', edit_employee_path(employee) %></td>
      <%# <td><%= link_to 'Delete', edit_employee_path(employee) %>
      <td><%= link_to 'Delete', employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>



